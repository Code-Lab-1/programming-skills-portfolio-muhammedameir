rivers = {
    'Sepik river': 'New Guinea',
    'volga river': 'russia',
    'zambesi': 'zambia',
    'nile': 'egypt',
    'yangtze': 'china',
    }

for river, country in rivers.items():
    print(f"The {river.title()} runs through {country.title()}.")

print("\nThe rivers mentioned in this data set:")
for river in rivers.keys():
    print(f"- {river.title()}")

print("\nThe rivers below are from the following countries :")
for country in rivers.values():
    print(f"- {country.title()}")
